{
	"extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier", "tslint-react-a11y"],
	"linterOptions": {
		"exclude": ["config/**/*.js", "node_modules/**/*.ts", "coverage/lcov-report/*.js", "*.d.ts"]
	},
	"rules": {
		"no-var-requires": false,
		"ordered-imports": false,
		"object-literal-sort-keys": false,
		"react-a11y-anchors": false
	}
}
